{
  "version": "0.1",
  "metadata": {
    "name": "Fonden Reconstrucción",
    "description": "Registros de Reconstrucción del Fondo Nacional de Desastres Naturales",
    "author": "Eduardo Clark",
    "organization": "Fondo Nacional de Desastres Naturales",
    "website": "datos.gob.mx",
    "email": "contacto@datos.gob.mx",
    "phone": ""
  },
  "data": {
    "type": "csv",
    "options": {
      "separator": ","
    }
  },
  "storage": {
    "type": "mongodb",
    "options": {
      "collection": "fonden.reconstruccion",
      "index": "cve"
    }
  },
  "errors": {
    "type": "stdout"
  },
  "hotspot": {
    "type": "tcp"
  },
  "extras": {
    "docker": {
      "image": "mxabierto/buda-agent-csv"
    }
  }
}